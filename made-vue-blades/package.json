    "postcss": "^8.3.11",
    "rollup-plugin-copy": "^3.4.0",
    "sass": "^1.43.4",
    "tailwindcss": "^2.2.19",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3",
    "vite": "^2.6.13",
    "vite-plugin-dts": "^0.9.2",
    "vue-loader": "^16.8.2",
