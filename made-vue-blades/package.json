  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.6",
    "@vitejs/plugin-vue": "^1.9.4",
    "@vue/compiler-sfc": "^3.2.21",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
